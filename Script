   AU = Tab1:AddToggle({
      Name = "Farm Grave",
      Default = false,
      Callback = function(Value)
         _G.FG = Value
         while _G.FG do
            local textValue = game.Players.LocalPlayer.Character.Head.Nametag.TextLabel.Text
            local killCount = tonumber(textValue)
            local gravestone = game.Workspace:FindFirstChild("Gravestone")
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Killstreak" then
               if killCount and killCount == 10 then

                  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace["Safespot"].CFrame * CFrame.new(0,10,0)

                  if gravestone then
                     fireclickdetector(gravestone.ClickDetector)
                  else
                     -- print("hello")
                  end
               else
                  OrionLib:MakeNotification({Name = "Error",Content = "you have <10 kills",Image = "rbxassetid://7733658504",Time = 5})
                  task.wait(0.5)
                  AU:Set(false)
               end
            else
               OrionLib:MakeNotification({Name = "Error",Content = "you don't equipped killstreak",Image = "rbxassetid://7733658504",Time = 5})
               task.wait(0.5)
               AU:Set(false)
            end
            task.wait()
         end  
      end
   })

